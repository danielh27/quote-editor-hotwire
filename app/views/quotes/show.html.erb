<main class="container">
  <%= link_to "Back to quotes", quotes_path %>

  <div class="header">
    <h1>
      <%= @quote.name %>
    </h1>

    <%= link_to "New Date",
      new_quote_item_date_path(@quote),
      class: "btn btn--primary" %>
      
  </div>

  <%= render @item_dates, quote: @quote %>
</main>
